<html>

<head>
    <meta charset='utf-8'>
    <meta http-equiv='X-UA-Compatible' content='IE=edge'>
    <title>JS Validation - Click Event with custom Email validation</title>
    <meta name='viewport' content='width=device-width, initial-scale=1'>
    <link rel='stylesheet' href='css/bootstrap.min.css'>
    <link rel='stylesheet' href='css/style.css'>

</head>

<body>
    <div class="container">
        <form action="" method="POST" id="frmRegisterUser">
            <div class="form-group">
                <h3>Company Info</h3>
            </div>

            <div class="row">
                <div class="col-md-3">
                    <div class="form-group">
                        <label>Company Name <span class="text-danger">*</span></label>
                        <input type="text" class="form-control" value="Payru Remit" id="txtCompanyName">
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label>Parter ID</label>
                        <input type="text" class="form-control clsPartnerID" value="PR" id="txtPartnerID">
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label>Partner Type<span class="text-danger">*</span></label>
                        <select class="form-control" value=" " id="ddlPartnerType">
                            <option>Sender Agent</option>
                            <option>Sleeping Partner</option>
                            <option>Business Partner</option>
                            <option>Limited Partner</option>
                            <option>Outgoing Partner</option>
                        </select>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label>Business PAN/VAT#<span class="text-danger">*</span></label>
                        <input type="text" class="form-control" id="txtBusinessPAN/VAT#">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-3">
                    <div class="form-group">
                        <label>Address<span class="text-danger">*</span></label>
                        <input type="text" class="form-control" value="136 Shirley Street" id="txtAddress">
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label>City<span Class="text-danger">*</span></label>
                        <input type="text" class="form-control" value="PIMPAMA" id="txtCity">
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label>Country<span Class="text-danger">*</span></label>
                        <select class="form-control" id="ddlCountry">
                            <option>Australia</option>
                            <option>America</option>
                            <option>South Africa</option>
                            <option>United Kingdom</option>
                            <option>NewZealand</option>
                        </select>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label>Local Currency<span class="text-danger">*</span></label>
                        <select class="form-control" id="ddlCountry">
                            <option>AUD</option>
                            <option>USD</option>
                            <option>EURO</option>
                            <option>GBP</option>
                        </select>
                    </div>
                </div>

                <label class="text-danger" id="lblName"></label>
            </div>
            <div class="row">
                <div class="col-md-3">
                    <div class="form-group">
                        <label>Phone<span Class="text-danger">*</span></label>
                        <input type="text" name="name" class="form-control" value="1234567890" id="txtPhone">
                        <label class="text-danger" id="lblPhone"></label>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label>Fax</label>
                        <input type="text" class="form-control" id="txtFax">
                        <label class="text-danger" id="lblFax"></label>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label>PinNo Prefix</label>
                        <input type="text" class="form-control" value="PR" id="txtPinNoPrefix">
                        <label class="text-danger" id="lblPinNoPrefix"></label>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label>Email</label>
                        <input type="text" class="form-control" id="txtEmail">
                        <label class="text-danger" id="lblEmail"></label>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-3">
                    <div class="form-group">
                        <label>GMT Time Zone<span Class="text-danger">*</span></label>
                        <select class="form-control" id="ddlGMTTimeZone">
                            <option>(GMT + 10.00)Brisane,Sydney</option>
                            <option>(GMT + 10.00)Annandale,Virginia</option>
                            <option>(GMT + 10.00)Los Angeles,California</option>
                            <option>(GMT + 10.00)Nicosia,Cyprus</option>
                        </select>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group" style="margin-top: 35px;">
                        <label>
                            <input type="checkbox"> Block this Partner
                        </label>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <h3>Contact Info</h3>
            </div>

            <div class="row">
                <div class="col-md-3">
                    <div class="form-group">
                        <label>Contact Name <span class="text-danger">*</span> <em>eg. First Name</em></label>
                        <input type="text" class="form-control" value="Payru" id="txtContactName">
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label>Post<span Class="text-danger">*</span></label>
                        <input type="text" class="form-control clsPost" value="Manager" id="txtPost">
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label>Email<span class="text-danger">*</span></label>
                        <input type="text" class="form-control" value="payru@payru.remit.au" id="txtEmail">
                    </div>
                </div>
                <div class="col-md-3">
                    <label>Partner Logo</label>
                    <img src="~/Image/photo">
                </div>
            </div>
            <div class="row">
                <div class="col-md-3">
                    <div class="form-group">
                        <label>Contact Name</label>
                        <input type="text" class="form-control" value="" id="txtContactName">
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label>Post</label>
                        <input type="text" class="form-control clsPost" value="" id="txtPost">
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label>Email</label>
                        <input type="text" class="form-control" id="txtEmail">
                    </div>
                </div>
            </div>




        </form>
    </div>

</body>


</html>